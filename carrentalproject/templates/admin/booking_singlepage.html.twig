{% if bookingList %}
    {% for booking in bookingList %}
        <tr>
            <td>{{booking.id}}</td>
            <td>{{booking.userId}}</td>
            <td>{{booking.carId}}</td>
            <td>{{booking.rateType}}</td>
            <td>{{booking.rateValue}}</td>
            <td>{{booking.reservationDate}}</td>
            <td>{{booking.pickupDate}}</td>
            <td>{{booking.returnDate}}</td>
            <td>{{booking.paymentAmount}}</td>
            <td>{{booking.bookingStatus}}</td>
            <td>
            <div class="btn-group">
                <a class="btn btn-success" href="/admin/booking/confirm/{{booking.id}}"><i class="fas fa-check"></i></a>
                <a class="btn btn-danger" href="/admin/booking/cancel/{{booking.id}}"><i class="fas fa-times"></i>

</a>
            </div>
            </td>
        </tr>
    {% endfor %}            
{% endif %}     


